{
  "name": "@thesatoshicompany/ontraport-js",
  "version": "0.1.8",
  "author": "Derek Barrera <derekbarrera@gmail.com>",
  "license": "Apache-2.0",
  "description": "Ontraport Client for Javascript written in Typescript", 
  "engines": {
    "node": ">= 12.13 <13"
  },
  "main": "build/src/main.js",
  "scripts": {
    "start": "node --inspect -r ts-node/register src/index.ts",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "qs": "^6.9.1",
    "tslib": "~1.10.0"
  },
  "devDependencies": {
    "@types/jest": "~24.0.25",
    "@types/node": "~12.12.22",
    "@typescript-eslint/eslint-plugin": "~2.14.0",
    "@typescript-eslint/parser": "~2.14.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.9.0",
    "eslint-plugin-jest": "~23.2.0",
    "jest": "~24.9.0",
    "prettier": "~1.19.1",
    "rimraf": "~3.0.0",
    "ts-jest": "~24.2.0",
    "ts-node": "^8.6.2",
    "tsutils": "~3.17.0",
    "typescript": "~3.7.4"
  }
}
